{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "barryvdh/laravel-snappy": "^1.0",
        "guzzlehttp/guzzle": "^7.2",
        "jackiedo/dotenv-editor": "^2.1",
        "jenssegers/agent": "^2.6",
        "kyon147/laravel-shopify": "^21.1",
        "laravel/cashier": "^14.12",
        "laravel/fortify": "^1.18",
        "laravel/framework": "^10.38",
        "laravel/horizon": "^5.21",
        "laravel/octane": "^2.3",
        "laravel/pulse": "^1.0@beta",
        "laravel/sanctum": "^3.2",
        "laravel/socialite": "^5.11",
        "laravel/telescope": "^4.17",
        "laravel/tinker": "^2.8",
        "league/flysystem-aws-s3-v3": "^3.0",
        "maatwebsite/excel": "^3.1",
        "php-flasher/flasher-laravel": "^1.15",
        "predis/predis": "^2.2",
        "socialiteproviders/shopify": "^4.1",
        "spatie/browsershot": "^3.59",
        "spatie/laravel-activitylog": "^4.7",
        "spatie/laravel-analytics": "^5.1",
        "spatie/laravel-medialibrary": "^10.14",
        "spatie/laravel-permission": "^6.0",
        "tightenco/ziggy": "^1.8",
        "yajra/laravel-datatables": "^10.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.9",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
         "install-ignore-platform-reqs": "composer install --ignore-platform-reqs",
        "update-ignore-platform-reqs": "composer update --ignore-platform-reqs"

    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
